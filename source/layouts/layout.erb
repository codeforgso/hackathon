<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- jquery -->
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- typekit -->
    <script src="https://use.typekit.net/xmz8vun.js"></script>
    <script>try{Typekit.load({ async: true });}catch(e){}</script>

    <!-- pull in Font Awesome CDN -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Code for Greensboro Hackathon Fall 2015" %></title>

    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag  "all" %>
    <meta name="viewport" content="initial-scale=1">
  </head>

  <body class="<%= page_classes %>">

    <div class="menu-fade-screen"></div>

    <%= partial "partials/navigation" %>

    <%= yield %>

    <%= partial "partials/footer" %>
  </body>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-31602595-13', 'auto');
    ga('send', 'pageview');

  </script>
</html>
